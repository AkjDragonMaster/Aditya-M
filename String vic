#include <stdio.h>

int main()
{
    int b=0;
    char input[9], ch[9] = "chocolate"; 
    printf("Enter text: ");
    scanf("%s", input);
    for(int i = 0; i < 9; i++){
        if(input[i] == ch[i]){
            b++;
        }
    }
    if(b == 8){
        printf("Search result found");
    }else{
        printf("Search result not found");
    }

    return 0;
}
